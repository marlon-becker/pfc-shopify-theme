body{ 
  font-family: 'ProximaNovaRegular', sans-serif;
}

/* Theme styles */

body {
  font-size: {{ settings.font_size_base }}px;
  line-height: 1.6em;
  color: {{ settings.font_col_base }};
  background: {{ settings.foot_bg_col }};
  letter-spacing: {{ settings.letter_sp }};
  overflow-y: scroll;
}

::selection{/* Webkit & Opera */
  background:{{ settings.font_col_heads }};
  color:{{ settings.bg_col }};
}
::-moz-selection{ /* Firefox */
  background:{{ settings.font_col_heads }};
  color:{{ settings.bg_col }};
}

a,
a:link,
a:visited {
  color:{{ settings.font_col_link }};
  -moz-transition: color 100ms;
  -webkit-transition: color 100ms;
  transition: color 100ms;
  text-decoration:none;
}

a:hover {
  color:{{ settings.font_col_link_hover }};
}

select {
  max-width: 100%;
}

input,
textarea {
  font-size: {{ settings.font_size_base }}px;
  line-height: {{ settings.font_size_base }}px;
  background-color: {{ settings.font_col_input_bg }};
  color: {{ settings.font_col_base }};
  border: 1px solid {{ settings.font_col_input_bord }};
  padding: 11px 15px;
  margin: 0;
  vertical-align: middle;
  max-width: 100%;
  letter-spacing: {{ settings.letter_sp }};
}

input[type=checkbox] {
  border:none;
  -moz-border-radius:0;
  -webkit-border-radius:0;
  border-radius:0;
}

textarea {
  height:120px;
  padding-top: 10px;
  padding-bottom: 10px;
}

/* HTML5 input placeholders */
::-webkit-input-placeholder {
  {% if settings.font_headnav_uc %}
  text-transform: uppercase;
  {% endif %}
  color: {{ settings.font_col_input }};
}

:-moz-placeholder { /* Firefox 18- */
  {% if settings.font_headnav_uc %}
  text-transform: uppercase;
  {% endif %}
  color: {{ settings.font_col_input }}; 
}

::-moz-placeholder {  /* Firefox 19+ */
  {% if settings.font_headnav_uc %}
  text-transform: uppercase;
  {% endif %}
  color: {{ settings.font_col_input }};
}

:-ms-input-placeholder {
  {% if settings.font_headnav_uc %}
  text-transform: uppercase;
  {% endif %}
  color: {{ settings.font_col_input }};
}

.button,
a.button,
button,
input[type="submit"],
input[type="reset"],
input[type="button"] {
  background:{{ settings.btn_col }};
  border: none;
  color:{{ settings.btn_col_txt }};
  font-size: {{ settings.font_size_special_but }}px;
  line-height: {{ settings.font_size_special_but }}px;
  height: auto;
  margin: 0;
  text-decoration: none !important;
  {% if settings.font_headnav_uc %}
  text-transform: uppercase;
  {% endif %}
  cursor: pointer;
  padding: 16px 15px 14px;
  vertical-align: middle;
  min-width: 120px;
  text-align: center;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
  -moz-border-radius:0;
  -webkit-border-radius:0;
  border-radius:0;
  -moz-transition: background-color 100ms, color 100ms;
  -webkit-transition: background-color 100ms, color 100ms;
  transition: background-color 100ms, color 100ms;
  display: inline-block;
  zoom:1;
  *display:inline;
  letter-spacing: {{ settings.letter_sp }};
}

.button.compact,
button.compact,
input[type="submit"].compact,
input[type="reset"].compact,
input[type="button"].compact {
  width: auto;
  min-width: 0;
}

.button:hover,
button:hover,
input[type="submit"]:hover,
input[type="reset"]:hover,
input[type="button"]:hover {
  background: {{ settings.btn_col_alt }};
}

.button.altcolour,
button.altcolour,
input[type="submit"].altcolour,
input[type="reset"].altcolour,
input[type="button"].altcolour {
  background: {{ settings.btn_col_txt }};
  color: {{ settings.btn_col }};
}

.button.altaltcolour,
button.altaltcolour,
input[type="submit"].altaltcolour {
  background: {{ settings.btn_col_alt }};
}

.button.altaltcolour:hover,
button.altaltcolour:hover,
input[type="submit"].altaltcolour:hover {
  background: {{ settings.btn_col }};
}

.button.notabutton,
a.button.notabutton,
button.notabutton,
input[type="submit"].notabutton {
  background: none;
  border: none;
  padding: 0;
  margin: 0;
  font-size: inherit;
  line-height: inherit;
  font-family: inherit;
  letter-spacing: inherit;
  text-transform: none;
  vertical-align: baseline;
  min-width: 0;
  overflow: visible;
  height: auto;
  color:{{ settings.font_col_link }};
  -moz-transition: color 100ms;
  -webkit-transition: color 100ms;
  transition: color 100ms;
}

.button.notabutton:hover,
a.button.notabutton:hover,
button.notabutton:hover,
input[type="submit"].notabutton:hover {
  color:{{ settings.font_col_link_hover }};
}

.button.disabled,
a.button.disabled,
button[disabled],
input[type="submit"][disabled],
input[type="reset"][disabled],
input[type="button"][disabled] {
  background: #999 !important;
}

h1, h2, h3, h4, h5, h6 {
  color: {{ settings.font_col_heads }};
  font-weight: normal;
  line-height: 1.2em;
  {% if settings.font_headnav_uc %}
  text-transform: uppercase;
  {% endif %}
  margin-bottom: 1em;
}

h1 a, h2 a, h3 a, h4 a, h5 a, h6 a,
h1 a:link, h2 a:link, h3 a:link, h4 a:link, h5 a:link, h6 a:link,
h1 a:visited, h2 a:visited, h3 a:visited, h4 a:visited, h5 a:visited, h6 a:visited {
  color: inherit;
  text-decoration: none;
}

h1 a:hover, h2 a:hover, h3 a:hover, h4 a:hover, h5 a:hover, h6 a:hover {
  color:{{ settings.font_col_link_hover }};
}

h1,
h2.h1-style { font-size: {{ settings.font_size_headings }}px; }

{% assign heading_inc = settings.font_size_headings | minus: settings.font_size_base | divided_by: 6 %}
{% for i in (2..6) %}

h{{i}},
h1.h{{i}}-style,
h2.h{{i}}-style,
h3.h{{i}}-style,
h4.h{{i}}-style,
h5.h{{i}}-style,
h6.h{{i}}-style {
  font-size: {{ 7 | minus: i | times: heading_inc | plus: settings.font_size_base }}px;
}
{% endfor %}

h1.majortitle,
h2.title, /* Req for Shopify account templates */
h2.majortitle,
h3.majortitle,
h4.majortitle,
h5.majortitle,
h6.majortitle,
.majortitle {
  margin: 10px 0;
}

h1.hometitle,
h2.hometitle,
h3.hometitle,
.hometitle {
  line-height: 1em;
  margin: 25px 0;
}


img {
  max-width: 100%;
  height: auto;
}

.hidden {
  display: none;
}

/* Lists  */
#pageheader ul,
#pageheader li {
  margin: 0;
  padding: 0;
  display: inline;
}

.container {
  max-width: {{ settings.adv_page_width }}px;
  margin-left: auto;
  margin-right: auto;
  padding-left: 40px;
  padding-right: 40px;
}

.container .container {
  padding-left: 0;
  padding-right: 0;
}

.flexslider .container {
  padding-left: 40px;
  padding-right: 40px;
}

#content {
  background: {{ settings.bg_col }};
  
  {% unless settings.bg_img_name == 'none' %}
  background-image:url('{{ settings.bg_img_name | asset_url }}');
  {% endunless %}
  
  {% if settings.bg_method contains 'repeatxy' %}
  background-repeat:repeat;
  {% elsif settings.bg_method contains 'repeatx' %}
  background-repeat:repeat-x;
  {% elsif settings.bg_method contains 'norepeat' %}
  background-repeat:no-repeat;
  {% endif %}
  {% if settings.bg_method contains 'centertop' %}
  background-position:center top;
  {% endif %}
  
  padding-bottom: 60px;
}

.template-index #content {
  padding-bottom: 0;
}


/* Header */

#mobile-header {
  display: none;
  text-align: right;
  background: {{ settings.head_bg_col }};
  color: {{ settings.head_txt_col }};
  border-bottom: 1px solid {{ settings.head_nav_div_col }};
  font-size: 15px;
  line-height: 32px;
}

#mobile-header a {
  color: inherit;
}

#nav-collapse .mobile-nav-toggle {
  text-transform: uppercase;
  float: left;
  display: inline-block;
  position: relative;
  padding: 0 1em 0 2em;
  color: inherit;
}

#nav-collapse .mobile-nav-toggle span {
  position: absolute;
  background: #202020;
  left: 0.5em;
  width: 1em;
  height: 3px;
  top: 0;
  margin-top: 9px;
}

#mobile-header .mobile-nav-toggle span:nth-child(1) { top: 5px; }
#mobile-header .mobile-nav-toggle span:nth-child(2) { top: 10px; }

#mobile-nav-return {
  z-index: 7000;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 0;
  background: rgba(0,0,0,0.3);
  opacity: 0;
  transition: opacity 250ms;
}

#main-nav .mobile-features {
  display: none;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
}

#main-nav .mobile-features .mobile-nav-toggle {
  position: absolute;
  z-index: -1;
  top: 0;
  left: 200px;
  height: 100%;
  padding-top: 0;
  padding-bottom: 0;
  opacity: 0;
  transition: opacity 500ms, left 500ms;
}

#main-nav .mobile-features .mobile-search input[type=text] {
  box-sizing: border-box;
  width: 100%;
  border: none;
  height: 40px;
}

#main-nav .mobile-features .mobile-search input[type=submit] {
  position: absolute;
  top: 0;
  right: 0;
  height: 100%;
  width: 35px;
}

.fixed-header { position: fixed; top: 0; left: 0; width: 100%; z-index: 999999; }

#pageheader {
  position: relative;
  height: 80px;
  box-sizing: border-box;
  z-index: 4000; /* Place above slider for live-search dropdown */
  background-color: rgba(32, 32, 32, 0.9);
  border-bottom: 4px solid #fee833;
  transition: all 0.3s; 
  -webkit-transition: all 0.3s;
}

#pageheader,
#pageheader a {
  color: #fff;
  font-family: 'ProximaNovaLight', sans-serif;
}

#pageheader .logo {
  text-align: center;
  background: #fee833;
}

#pageheader .nav-container {
  margin: 0 auto;
  width: 100%;
}

#pageheader .main-nav-left {
  display: inline-block;
  float: left;
  width: calc(50% - 178px);
  text-align: right;
}

#pageheader .main-nav-right {
  display: inline-block;
  float: left;
  width: calc(50% - 178px);
  text-align: left;
}

#pageheader .main-nav-left li a, #pageheader .main-nav-right li a {
  color: rgba(255, 255, 255, 0.8);
  margin: 0 15px;
  padding: 6px 0;
  line-height: 34px;
  border-top: 2px solid rgba(255, 255, 255, 0);
  border-bottom: 2px solid rgba(255, 255, 255, 0);
  -webkit-transition: 0.3s;
  -moz-transition: 0.3s;
  -o-transition: 0.3s;
  -ms-transition: 0.3s;
  transition: 0.3s;
}

#pageheader .main-nav-left li, #pageheader .main-nav-right li {
  display: inline-block;
  padding: 24px 18px;
}

#pageheader .main-nav-right li:nth-child(3) a {
  border-top: 2px solid rgba(255, 255, 255, 0.8);
  border-bottom: 2px solid rgba(255, 255, 255, 0.8);
}

#pageheader .main-nav-left li:hover a, #pageheader .main-nav-right li:hover a{
  border-top: 2px solid rgba(254, 232, 51, 0.8);
  border-bottom: 2px solid rgba(254, 232, 51, 0.8);
  background: none;
}

#pageheader .main-nav-left {
  padding-right: 18px;
}

#pageheader .main-nav-right {
  padding-left: 18px;
}


#pageheader .logo-container {
  width: 320px;
/*  margin-left: auto;
  margin-right: auto;*/
  float: left;
  display: inline-block;
}

#pageheader .logo a {
  color: {{ settings.font_col_shoptitle }};
  line-height: 80px;
}

#pageheader .logo img {
  vertical-align: middle;
}

#pageheader .logo .logotext {
  font-size: {{ settings.font_size_shoptitle }}px;
  line-height: 1em;
}

#pageheader .search-box,
#pageheader .utils {
  position: absolute;
  top: 50%;
}

#pageheader .logo-pos-below .search-box,
#pageheader .logo-pos-below .utils,
#pageheader .logo-pos-above .search-box,
#pageheader .logo-pos-above .utils {
  top: 0;
  margin-top: 0;
}

#pageheader .search-box {
  left: 30px;
  margin-top: -15px;
}

#pageheader .search-box .search-form {
  border: 1px solid transparent;
  position: relative;
  display:inline-block;
  zoom:1;
  *display:inline;
}

#pageheader .search-box.focus .search-form {
  background-color: #fff;
  border-color: #dedede;
}

#pageheader .search-box .search-form i {
  width: 22px;
  height: 22px;
  background: url('{{ settings.social_col | asset_url }}') -221px center no-repeat;
  top: 7px;
  left: 2px;
  position: absolute;
  z-index: 1;
}

#pageheader .search-box.focus .search-form i {
  background-image: url('{{ 'social_spr_darkgrey.png' | asset_url }}');
}

#pageheader .search-box .search-form input[type="text"] {
  border: none;
  background: transparent;
  padding: 11px 10px 11px 26px;
  width: 210px;
  font-size: {{ settings.font_size_special_head }}px;
  color: {{ settings.head_txt_col }};
}

#pageheader .search-box.focus .search-form input[type="text"] {
    color: #666;
}

/* HTML5 input placeholders */
#pageheader .search-box .search-form input[type="text"]::-webkit-input-placeholder {
  color: {{ settings.head_txt_col }};
  opacity: 1;
}

#pageheader .search-box .search-form input[type="text"]:-moz-placeholder { /* Firefox 18- */
  color: {{ settings.head_txt_col }}; 
  opacity: 1;
}

#pageheader .search-box .search-form input[type="text"]::-moz-placeholder {  /* Firefox 19+ */
  color: {{ settings.head_txt_col }};
  opacity: 1;
}

#pageheader .search-box .search-form input[type="text"]:-ms-input-placeholder {
  color: {{ settings.head_txt_col }};
  opacity: 1;
}

#pageheader .search-box .search-form input[type="submit"] {
  opacity: 0;
  filter:alpha(opacity=0);
  position: absolute;
  top: 0;
  right: 8px;
  background: transparent;
  color: {{ settings.font_col_base }};
  border: none;
  width: 20px;
  min-width: 0;
  height: 25px;
  padding: 5px 0;
  line-height: 2px;
  font-size: 100%;
}

#pageheader .search-box.focus .search-form input[type="submit"] {
  opacity: 1;
  filter:alpha(opacity=100);
}

#pageheader .search-box .results-box {
  position: absolute;
  background: #fff;
  color: #333;
  width: 300px;
  max-width: 100%;
  z-index: 5000;
}

#pageheader .search-box .results-box a,
#pageheader .search-box .results-box a:hover {
  color: inherit;
}

#pageheader .search-box .results-box > * {
  display: block;
  padding: 7px 2px;
  margin: 0;
  border-bottom: 1px solid {{ settings.col_dividers }};
  border-left: 1px solid {{ settings.col_dividers }};
  border-right: 1px solid {{ settings.col_dividers }};
  min-height: 24px;
}

#pageheader .search-box .results-box > *:first-child {
  border-top: 1px solid {{ settings.col_dividers }};
}

#pageheader .search-box .results-box > *:hover {
}

#pageheader .search-box .results-box .img {
  float: left;
  margin: -4px 10px 0 0;
  width: 32px;
  height: 32px;
  text-align: center;
  overflow: hidden;
}

#pageheader .search-box .results-box .img img {
  vertical-align: middle;
  margin-top: -2px;
}

#pageheader .search-box .results-box .load {
  background: url('{{ 'ajax-load.gif' | asset_url }}') center center no-repeat;
  text-align: center;
}

#pageheader .search-box .results-box .note {
  text-align: center;
  background:{{ settings.btn_col }};
  border: {{ settings.btn_col }};
  color:{{ settings.btn_col_txt }};
  font-size: {{ settings.font_size_special_but }}px;
  {% if settings.font_headnav_uc %}
  text-transform: uppercase;
  {% endif %}
}

#pageheader .utils {
  right: 30px;
  height: 44px;
  line-height: 48px;
  font-size: 15px;
  margin-top: -24px;
}

#pageheader .utils .social-links,
#pageheader .utils .cart-summary {
  vertical-align: middle;
  display:inline-block;
  zoom:1;
  *display:inline;
}

#pageheader .utils .social-links {
  margin-right: 30px;
}

#pageheader .utils .cart-summary {
  text-align: right;
}

#pageheader .utils .cart-summary .switcher {
  margin-left: 3px;
  display: inline;
  vertical-align: middle;
  position: relative;
}

#pageheader .utils .cart-summary .switcher .selected-currency {
  padding: 4px 5px;
  border: 1px solid {{ settings.head_nav_div_col }};
  cursor: pointer;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  border-radius: 2px;
}

#pageheader .utils .cart-summary .switcher .switcher-drop {
  display: none;
  position: absolute;
  right: 0;
  top: 100%;
  z-index: 50;
}

#pageheader .utils .cart-summary .switcher .switcher-drop .inner {
  margin-top: 1.3em;
  line-height: 1em;
  padding: 8px 20px 6px;
  border: 1px solid #dedede;
  background: #fff;
  color: #444;
  white-space: nowrap;
}

#pageheader .utils .cart-summary .switcher .switcher-drop .inner:before {
  content: " ";
  width: 0px;
  height: 0px;
  border-style: solid;
  border-width: 0 6px 6px 6px;
  border-color: transparent transparent #dedede transparent;
  position: absolute;
  top: 1.3em;
  right: 15px;
  margin-top: -5px;
}

#pageheader .utils .cart-summary .switcher .switcher-drop .inner:after {
  content: " ";
  width: 0px;
  height: 0px;
  border-style: solid;
  border-width: 0 6px 6px 6px;
  border-color: transparent transparent #fff transparent;
  position: absolute;
  top: 1.3em;
  right: 15px;
  margin-top: -4px;
